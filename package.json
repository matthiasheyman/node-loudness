    "ava": "^4.2.0",
    "del-cli": "^4.0.1",
    "np": "^7.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "xo": "^0.48.0"
  },
  "engines": {
    "node": ">=16"
  },
  "ava": {
    "extensions": {
